# Copy this file to terraform.tfvars and fill in your values

# Your GCP project ID
project_id = "your-project-id-here"

# Your GCP project number (find with: gcloud projects describe PROJECT_ID --format="value(projectNumber)")
project_number = "123456789012"

# Your domain name (without subdomain)
# The broadcaster application will be accessible at broadcaster.yourdomain.com
domain = "example.com"

# GitHub App installation ID (find this in GitHub Settings → Applications → Installed GitHub Apps)
# Look for "Google Cloud Build" app and check the URL when you click "Configure"
# Example: if URL is https://github.com/settings/installations/12345678, then use "12345678"
github_app_installation_id = "12345678"

# GitHub repository configuration
# Your GitHub username or organization name
github_repo_owner = "your-github-username"

# Your GitHub repository name
github_repo_name = "multicluster-broadcaster-swm-l7mp"

# Your GitHub personal access token
# See https://docs.github.com/en/authentication/keeping-your-account-and-data-secure/managing-your-personal-access-tokens#creating-a-personal-access-token-classic
# Required scopes: repo, read:user (and read:org if installing in an organization)
github_pat = "ghp_12345abcde"